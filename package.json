    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "eslint": "^8.11.0",
    "husky": "^8.0.0",
    "lint-staged": "^12.3.5"
  },
  "rules": {
    "curly": [
      "warn",
      "all"
    ]
  },
  "lint-staged": {
    "*": "eslint --cache --fix"
  }
